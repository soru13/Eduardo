/*<<<<<<<<<<<<<<<<<<<<<<<Contact>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
.Contact-title
	font-weight  bold
	font-size  52px
.Contact-text
	max-width  835px
	margin  30px auto 0 auto
	font-size  21px
.Contact-medias
	margin  40px 0 0
	padding  0
	display  block
	list-style-type  disc
.Contact-media
	display  inline-block
	text-align  center
	margin-bottom  40px
	vertical-align  top
	margin  0
	width 15%
.Contact-mediaName
	text-align  center
	font-size  26px
	margin  0 0 20px
@media screen and (max-width  1200px)
	.Contact-media
		width 100%
/*<<<<<<<<<<<<<<<<<<<<<<<end Contact>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
/*<<<<<<<<<<<<<<<<<<<<<<<growht myKnowledgeStrategy>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
.tools
	flex-flow  wrap
	justify-content  space-between
	overflow  hidden
	padding  0
	text-align  left
	center
		color  purple
	li
		margin-bottom  1%
	img,svg
		width 200px
		height 41px
		display inline-block
	p
		display inline-block
		margin-left 1%
