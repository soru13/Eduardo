vendor-prefixes = o ms moz webkit
body
	font-family: 'Lato',sans-serif;
	text-align:center;
footer
	color white
	background-color black
#videoAirbnb
	position: relative;
	width: 100%;
	display:none;
#imagPrincipal
	position: relative;
	width: 100%;
	height: 546px;
	background-image:url('https://raw.githubusercontent.com/soru13/Eduardo/gh-pages/img/javascriptpro_cover.jpg');
#header
	position:absolute;
	text-align:center;
	width: 100%;
	top: 0px;
	#blockLanding
		padding-top: 130px;
	#mynameTitle
		color: white;
		line-height: .9em;
		margin: 0px;
		position: relative;
	.myCopys
		color: white;
		line-height: .9em;
		margin: 0px;
		padding: 0px;
		position: relative;
		margin-top: 22px;
	#content
		position: relative;
		color:white;
	#imgDevices
		position: relative;
		width: 200px;//para pantalla chica
	#btnMoree
		position: relative;
		border-radius: 0px;
		background: rgba(255,255,255,0.5);
		border-style: none;
		&:hover
			background-color: rgba(4, 4, 4, 0.5);
/*media Query principal airbnb*/
@media screen and (max-width: 480px)
	#imgDevices
		display:none;
@media screen and (max-width: 640px)
	#mynameTitle
		font-size: 40px;//pantalla chica
	.myCopys
		font-size: 14px;//para pantalla chica
	#content
		font-size: 15px;//para pantalla chica
@media screen and (min-width: 1024px)
	#mynameTitle
		font-size: 70px;//pantalla grande
	.myCopys
		font-size: 24px;//para pantalla grande
	#content
		font-size: 25px;//para pantalla grande
	#videoAirbnb
		display:block
	#imagPrincipal
		display:none;

