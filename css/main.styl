/*<<<<<<<<<<<<<<<<<<<<<<<<Trust-list>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
.Trust-list
	margin 0
	padding 0
	text-align center
	.Trust-company
		display inline-block
		margin 15px 25px
		-webkit-filter grayscale(100%)
		&:hover
			-webkit-filter none
			-webkit-filter grayscale(0)
	.sprite-nueva
		width 100px
		height 90px
	.sprite-hcc
		width 144px
		height 75px
	.sprite-coppel
		width 125px
		height 65px
	.sprite-credilikeme
		width 125px
		height 60px
	.sprite-uas
		width 125px
		height 125px
