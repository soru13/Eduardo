/*<<<<<<<<<<<<<<<<<<<<<<<end imgFrontend>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
/*<<<<<<<<<<<<<<<<<<<<<<<myKnowledgeFrontend>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
.LandingSummary-container
	box-sizing: border-box;
	margin: 0 auto;
	max-width: 1000px;
	padding: 0 15px;
	position: relative;
	.LandingSkills
		padding: 20px 0;
		margin-left: -40px;
		.LandingSkills-item
			display: inline-block;
			margin-bottom: 20px;
			margin-left: 40px;
			width: 95px;
			vertical-align: top;
			.LandingSkills-image
				margin-bottom: 5px;
	.LandingSummary-list
		column-count: 3;
		flex-flow: wrap;
		justify-content: space-between;
		overflow: hidden;
		padding: 0;
		.LandingSummary-item
			list-style: none;
			text-align: left;
			padding: 5px;
			box-sizing: border-box;
			position: relative;
			&:before
				content:'.'
				position: absolute;
				left: 0;
				top: 0;
				font-weight: bold;
/*<<<<<<<<<<<<<<<<<<<<<<<end myKnowledgeFrontend>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/

