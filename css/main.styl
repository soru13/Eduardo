@import "animate.min.css"
@import "font-awesome/css/font-awesome.min.css"
@import "nib/vendor"
@import 'nib/normalize'

vendor-prefixes = o ms moz webkit

body
	font-family 'Lato',sans-serif
	text-align center
footer
	color white
	background-color black
#imagPrincipal
	position  relative
	width  100%
	height  546px
	img 
		width 1600px
		height 546px
#header
	position absolute
	text-align center
	width  100%
	top  0px
	#blockLanding
		padding-top  148px
	#mynameTitle
		color  #0a2e3d
		line-height  .9em
		margin  0px
		position  relative
		a
			text-decoration none
			&:hover
				text-decoration none
	.myCopys
		color  	#FF0000
		line-height  .9em
		margin  0px
		padding  0px
		position  relative
		margin-top  22px
	#content
		position  relative
		color #FF0000
	#btnMoree
		position  relative
		border-radius  0px
		background  rgba(93,124,147,0.5)
		border-style  none
		&:hover{background-color:  rgba(4, 4, 4, 0.5);}
/*media Query principal airbnb*/
@media screen and (max-width  640px)
	#mynameTitle
		font-size  25px//pantalla chica
	.myCopys
		font-size  14px//para pantalla chica
	#content
		font-size  15px//para pantalla chica
@media screen and (min-width  1024px)
	#mynameTitle
		font-size  52px//pantalla grande
	.myCopys
		font-size  24px//para pantalla grande
	#content
		font-size  25px//para pantalla grande
/*<<<<<<<<<<<<<<<<<<<<<<<<menu>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
#menu
	position fixed
	width 100%
	height 60px
	text-align center
	top  10px
	background  rgba(255,255,255,0.9)
	z-index  1
	a
		color black
		text-decoration none
		cursor pointer
		&:hover
			color gray
			cursor pointer
	.menu
		display inline-block
		vertical-align top
		margin-top  17px
		float right
		margin-right  3%
		&:hover
			cursor pointer
	#brand
		float  left
		margin-left  20px
	.brand
		width 250px
		height 60px
/*<<<<<<<<<<<<<<<<<<<<<<<<end menu>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
/*<<<<<<<<<<<<<<<<<<<<<<<<aboutme>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
#UX
	display block
	max-width 100% !important
	height 546px
	color white
	padding-top 152px
	background-repeat  no-repeat
	background-position  center
	background-image url('../img/ux.png')

/*<<<<<<<<<<<<<<<<<<<<<<<<end aboutme>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
/*<<<<<<<<<<<<<<<<<<<<<<<imgFrontend>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
#imgFrontend
	display block
	max-width 100% !important
	height 546px
	color white
	padding-top 152px
	background-repeat  no-repeat
	background-position  center
	background-image url('https://raw.githubusercontent.com/soru13/Eduardo/gh-pages/img/frontend_cover.jpg')
.landdingBanner
	max-width  750px
	width auto
	margin  0 auto
	text-align center
	height  202px
	position relative
	.LandingBanner-left
		display block
		height  202px
		position relative
		float left
		.LandingBadge-container
			position relative
			margin 0px
		p
			margin 0px
			padding 0px
	.landingBanner-right
		text-align left
		display block
		position relative
/*media Query principal airbnb*/
@media screen and (max-width  1023px)
	.yo
		display none
	#yophoto
		display none
	.landingBanner-right
		padding-left  168px
@media screen and (max-width  640px)
	.landingBanner-right
		float  left
		width  480px
		padding-left  35px
@media screen and (max-width  480px)
	.landingBanner-right
		width  300px
	.LandingBanner-left
		width  100px
	.LandingBadge-item
		width  100px
		height  100px
		h1
			margin-bottom  27px
	.block
		width  100%
@media screen and (min-width  320px)
	body
		font-size 14px
	#menu
		height  117px
	.landingBanner-right
		width  215px
		padding-left  6px
		h1
			margin-bottom  14px
	.LandingBanner-left
		width  80px
	.LandingBadge-item
		width  80px
		height  80px
	.block
		width  100%
@media screen and (min-width  640px)
	#menu
		height  60px
	.landingBanner-right
		float  left
		width  480px
		padding-left  35px
	.LandingBanner-left
		width 146px
@media screen and (min-width  1024px)
	body
		font-size 16px
	.landingBanner-right
		float right
		width  600px
		padding-left  57px
	.block
		width 30%
	.LandingBadge-item
		width  146px
		height  146px
/*<<<<<<<<<<<<<<<<<<<<<<<end imgFrontend>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
/*<<<<<<<<<<<<<<<<<<<<<<<myKnowledgeFrontend>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
.LandingSummary-container
	box-sizing  border-box
	margin  0 auto
	max-width  1000px
	padding  0 15px
	position  relative
	.LandingSkills
		padding  20px 0
		margin-left  -40px
		.LandingSkills-item-float-left
			display  inline-block
			margin-bottom  20px
			margin-left  40px
			width  395px
			vertical-align  top
			float left
			.LandingSkills-image
				margin-bottom  5px
		.LandingSkills-item
			display  inline-block
			margin-bottom  20px
			margin-left  40px
			width  95px
			vertical-align  top
			.LandingSkills-image
				margin-bottom  5px
	.LandingSummary-list
		column-count  3
		flex-flow  wrap
		justify-content  space-between
		overflow  hidden
		padding  0
		.LandingSummary-item
			list-style  none
			text-align  left
			padding  5px
			box-sizing  border-box
			position  relative
			& before
				content '.'
				position  absolute
				left  0
				top  0
				font-weight  bold
/*<<<<<<<<<<<<<<<<<<<<<<<end myKnowledgeFrontend>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
/*<<<<<<<<<<<<<<<<<<<<<<<imgBackend>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
